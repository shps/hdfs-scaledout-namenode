#!/bin/sh 
source <%= node[:ndb][:scripts_dir] %>/util/force.sh
MYSQL_SOCKET=`<%= @ndb_dir %>/scripts/get-mysql-socket.sh`
wait_pid_removed=10
ID=<%= @node_id %>
PID_FILE=<%= node[:ndb][:log_dir] %>/mysql_$ID.pid 
<%= node[:ndb][:scripts_dir] %>/util/kill-process.sh mysqld $PID_FILE 0 $FORCE
exit $?

